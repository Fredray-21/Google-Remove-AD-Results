{
  "name": "Google_remove_ad_results",
  "description": "Chrome extension that removes ads from google search results",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "./icons/16_icon.png",
    "32": "./icons/32_icon.png",
    "48": "./icons/48_icon.png",
    "128": "./icons/128_icon.png"
  }
}

